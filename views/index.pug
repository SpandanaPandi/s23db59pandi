extends layout

block content
  h1= title
  p Welcome to #{title}
  p Spandana Pandi
  button(type="button", class="btn btn-secondary",onclick='goFaster()') Press Me 
  div(id="faster") Increase the speed
  a(href='http://localhost:3000/transportation') transportation Link
  script.
    var mph = 20;
    var change = 1;
    function goFaster(){
      mph += change ;
      change ++ ;
      var div = document.getElementById('faster');
      div.textContent = `MPH ${mph} with change ${change}` ;
    }
    a(href="/transportation") View transportation
