extends layout
 
block content
  h1= title
  - var images_names=["Flower1.jpg","Flower2.jpg","Flower3.jpg","Flower4.jpg","Flower5.jpg"]
  - for(let i=0;i<3;i++) {
    - let rand=Math.floor(Math.random()*5)
    img(src=`/images/${images_names[rand]}`, height="300px" width="300px" ,alt=`images_names[${rand}]`)
    console.log(#{rand})
  - }